{
  "name": "@lenne.tech/ng-base",
  "version": "15.0.0-beta-4",
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "homepage": "https://lennetech.github.io/ng-base/",
  "license": "MIT",
  "scripts": {
    "build": "ng build --configuration production",
    "watch": "watch 'npm run build && cd ./dist/ng-base && yalc push --private' ./projects/ng-base",
    "check": "npm run format:fix && npm run lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"projects/ng-base/src/{app,environments,assets}/**/*{.ts,.js,.json,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "format:fixAll": "prettier --write src",
    "lint": "ng lint",
    "ng": "ng",
    "build:pack": "npm run build && cd dist/ng-base && npm pack",
    "publish": "npm run build && cd dist/ng-base && npm publish",
    "postinstall": "husky install .husky",
    "reinit": "rimraf package-lock.json && rimraf node_modules && npm cache clean --force && npm i",
    "start": "ng serve",
    "test": "ng test",
    "release": "standard-version && git push --follow-tags origin main",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major && git push --follow-tags origin main",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "predeploy": "npm run build-storybook",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook --no-dll -o ./docs",
    "deploy-storybook": "gh-pages -d docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.9",
    "@angular/common": "15.2.9",
    "@angular/compiler": "15.2.9",
    "@angular/core": "15.2.9",
    "@angular/forms": "15.2.9",
    "@angular/localize": "15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@angular/router": "15.2.9",
    "@apollo/client": "3.7.14",
    "@ng-bootstrap/ng-bootstrap": "14.1.1",
    "@popperjs/core": "2.11.7",
    "@storybook/theming": "7.0.9",
    "apollo-angular": "4.2.1",
    "bootstrap": "5.2.3",
    "compressorjs": "1.2.1",
    "graphql": "16.6.0",
    "graphql-ws": "5.12.1",
    "js-sha256": "0.9.0",
    "ngx-image-cropper": "6.3.3",
    "rxjs": "7.8.1",
    "tslib": "2.5.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.8",
    "@angular/compiler-cli": "15.2.9",
    "@babel/core": "7.21.8",
    "@compodoc/compodoc": "1.1.19",
    "@storybook/addon-actions": "7.0.9",
    "@storybook/addon-docs": "7.0.9",
    "@storybook/addon-essentials": "7.0.9",
    "@storybook/addon-interactions": "7.0.9",
    "@storybook/addon-links": "7.0.9",
    "@storybook/angular": "7.0.9",
    "@storybook/builder-webpack5": "7.0.9",
    "@storybook/manager-webpack5": "7.0.0-alpha.10",
    "@storybook/testing-library": "0.1.0",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "babel-loader": "9.1.2",
    "eslint": "8.40.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "44.0.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jasmine-core": "4.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "15.2.2",
    "ng-samurai": "3.0.0",
