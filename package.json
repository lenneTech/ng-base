{
  "name": "@lenne.tech/ng-base",
  "version": "14.30.0",
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "homepage": "https://lennetech.github.io/ng-base/",
  "license": "MIT",
  "scripts": {
    "build": "ng build --configuration production",
    "watch": "watch 'npm run build && cd ./dist/ng-base && yalc push --private' ./projects/ng-base",
    "check": "npm run format:fix && npm run lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"projects/ng-base/src/{app,environments,assets}/**/*{.ts,.js,.json,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "format:fixAll": "prettier --write src",
    "lint": "ng lint",
    "ng": "ng",
    "build:pack": "npm run build && cd dist/ng-base && npm pack",
    "publish": "npm run build && cd dist/ng-base && npm publish",
    "postinstall": "husky install .husky",
    "reinit": "rimraf package-lock.json && rimraf node_modules && npm cache clean --force && npm i",
    "start": "ng serve",
    "test": "ng test",
    "release": "standard-version && git push --follow-tags origin main",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major && git push --follow-tags origin main",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "predeploy": "npm run build-storybook",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook --no-dll -o ./docs",
    "deploy-storybook": "gh-pages -d docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/common": "14.2.12",
    "@angular/compiler": "14.2.12",
    "@angular/core": "14.2.12",
    "@angular/forms": "14.2.12",
    "@angular/platform-browser": "14.2.12",
    "@angular/platform-browser-dynamic": "14.2.12",
    "@angular/router": "14.2.12",
    "@apollo/client": "3.7.7",
    "@storybook/theming": "6.5.16",
    "apollo-angular": "4.2.0",
    "bootstrap": "5.2.3",
    "compressorjs": "1.1.1",
    "graphql": "16.6.0",
    "graphql-ws": "5.11.3",
    "js-sha256": "0.9.0",
    "ngx-image-cropper": "6.3.2",
    "rxjs": "7.8.0",
    "tslib": "2.5.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "14.2.12",
    "@babel/core": "7.20.12",
    "@compodoc/compodoc": "1.1.19",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/angular": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "babel-loader": "9.1.2",
    "eslint": "8.33.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "39.8.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "gh-pages": "4.0.0",
    "husky": "8.0.3",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "14.2.2",
    "ng-samurai": "3.0.0",
    "prettier": "2.8.3",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "standard-version": "9.5.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "watch": "1.0.2"
  }
}
