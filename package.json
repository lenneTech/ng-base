{
  "name": "ng-base",
  "version": "13.6.7",
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "homepage": "https://lennetech.github.io/ng-base/",
  "license": "MIT",
  "scripts": {
    "build": "ng build --configuration production",
    "check": "npm run format:fix && npm run lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"projects/ng-base/src/{app,environments,assets}/**/*{.ts,.js,.json,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "format:fixAll": "prettier --write src",
    "lint": "ng lint",
    "ng": "ng",
    "build:pack": "npm run build && cd dist/ng-base && npm pack",
    "publish": "npm run build && cd dist/ng-base && npm publish",
    "postinstall": "husky install .husky",
    "reinit": "rimraf package-lock.json && rimraf node_modules && npm cache clean --force && npm i",
    "start": "ng serve",
    "test": "ng test",
    "release": "standard-version && git push --follow-tags origin main",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major && git push --follow-tags origin main",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "predeploy": "npm run build-storybook",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook --no-dll -o ./docs",
    "deploy-storybook": "gh-pages -d docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.3",
    "@angular/common": "13.3.3",
    "@angular/compiler": "13.3.3",
    "@angular/core": "13.3.3",
    "@angular/forms": "13.3.3",
    "@angular/platform-browser": "13.3.3",
    "@angular/platform-browser-dynamic": "13.3.3",
    "@angular/router": "13.3.3",
    "@apollo/client": "3.5.10",
    "@storybook/theming": "6.4.22",
    "apollo-angular": "3.0.1",
    "bootstrap": "5.1.3",
    "graphql": "16.3.0",
    "graphql-ws": "5.8.2",
    "rxjs": "7.5.5",
    "tslib": "2.3.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.3",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.3",
    "@angular/compiler-cli": "13.3.3",
    "@babel/core": "7.17.9",
    "@compodoc/compodoc": "1.1.19",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-docs": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/angular": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/testing-library": "0.0.9",
    "@types/jasmine": "4.0.3",
    "@types/node": "17.0.24",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "babel-loader": "8.2.5",
    "eslint": "8.13.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.2.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "jasmine-core": "4.1.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.18",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "13.3.1",
    "ng-samurai": "3.0.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "standard-version": "9.3.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  }
}
