{
  "name": "@lenne.tech/ng-base",
  "version": "18.0.0",
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "homepage": "https://lennetech.github.io/ng-base/",
  "license": "MIT",
  "scripts": {
    "build": "ng build --configuration production",
    "watch": "watch 'npm run build && cd ./dist/ng-base && yalc push --private' ./projects/ng-base",
    "check": "npm run format:fix && npm run lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"projects/ng-base/src/{app,environments,assets}/**/*{.ts,.js,.json,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "format:fixAll": "prettier --write src",
    "lint": "ng lint",
    "ng": "ng",
    "build:pack": "npm run build && cd dist/ng-base && npm pack",
    "publish": "npm run build && cd dist/ng-base && npm publish",
    "postinstall": "husky install .husky",
    "reinit": "rimraf package-lock.json && rimraf node_modules && npm cache clean --force && npm i",
    "start": "ng serve",
    "test": "ng test",
    "release": "standard-version && git push --follow-tags origin main",
    "release:17": "standard-version && git push --follow-tags origin angular-17",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major && git push --follow-tags origin main",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "predeploy": "npm run build-storybook",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook --no-dll -o ./docs",
    "deploy-storybook": "gh-pages -d docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.0.2",
    "@angular/common": "18.0.2",
    "@angular/compiler": "18.0.2",
    "@angular/core": "18.0.2",
    "@angular/forms": "18.0.2",
    "@angular/localize": "18.0.2",
    "@angular/platform-browser": "18.0.2",
    "@angular/platform-browser-dynamic": "18.0.2",
    "@angular/router": "18.0.2",
    "@apollo/client": "3.10.4",
    "@popperjs/core": "2.11.8",
    "@storybook/theming": "8.1.6",
    "apollo-angular": "7.0.2",
    "bootstrap": "5.3.3",
    "compressorjs": "1.2.1",
    "graphql": "16.8.1",
    "graphql-ws": "5.16.0",
    "js-sha256": "0.11.0",
    "ngx-image-cropper": "8.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.3",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "18.0.3",
    "@angular/compiler-cli": "18.0.2",
    "@babel/core": "7.24.7",
    "@types/jasmine": "5.1.4",
    "@types/node": "20.14.2",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "babel-loader": "9.1.3",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.2.9",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "9.0.11",
    "jasmine-core": "5.1.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "18.0.0",
    "ng-samurai": "3.0.0",
    "prettier": "3.3.1",
    "pretty-quick": "4.0.0",
    "protractor": "7.0.0",
    "rimraf": "5.0.7",
    "standard-version": "9.5.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "watch": "1.0.2"
  },
  "overrides": {
    "@angular-eslint/eslint-plugin": {
      "@typescript-eslint/utils": "7.11.0"
    }
  }
}
